   "source": [
    "from math import floor\n",
    "def diamond(n):\n",
    "    n=int(n)\n",
    "    stapp =''\n",
    "    for each in range(n):\n",
    "        x = \" \"*floor(n/2)\n",
    "        y = \"*\"*int(n-2*floor(n/2))\n",
    "        stapp+=x+y+\" \\n\"\n",
    "    return stapp\n"
   ]
  },
